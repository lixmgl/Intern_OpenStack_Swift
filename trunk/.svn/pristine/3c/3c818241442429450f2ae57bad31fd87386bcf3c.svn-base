#
# BMC Impact Manager 8.5
# Generic CLI configuration
# Format : see mcell.conf
#

#Trace=Yes
#TraceSrc=No
#TraceProgram=Yes
#TraceModule=Yes
#TraceTime=Yes
#TraceConfigFileName=mclient.trace
#TraceDefaultFileName=%T/trace
#TraceFileSize=0
#TraceFileHistory=0
#TraceFileAppend=Yes
#TraceEvents=

#LocaleConfigFileName=%H/etc/locale/%P.load

#Encryption=Yes
#EncryptionKey=

#ServerName=
#ServerLocation=
#ServerDirectoryName=mcell.dir
#ServerPort=1828

#ConnectionSetupTimeOut=20
#ConnectionPortRange=
#ConnectionPortReuse=Yes
